{
  "name": "cassandra-ng",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "help": "ng help",
    "start": "ng serve",
    "build": "ng build",
    "start-ci": "ng serve --env=ci --host 0.0.0.0",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test -cc",
    "test-chrome": "ng test -cc -c karma-chrome.conf.js",
    "test-once": "ng test -cc --no-watch",
    "pree2e": "webdriver-manager update",
    "e2e": "ng e2e",
    "e2e-chrome": "ng e2e -c protractor-chrome.conf.js",
    "stubby": "stubby -d e2e/graphhopper.endpoints",
    "coveralls": "cat ./coverage/coverage.lcov | ./node_modules/coveralls/bin/coveralls.js",
    "hmr": "ng serve --hmr -e=hmr",
    "routes": "download --extract --out src/assets/routes https://github.com/roi-itlab/web/releases/download/1.0/routes.zip"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "@types/webpack-env": "^1.13.0",
    "bootstrap": "next",
    "core-js": "^2.4.1",
    "font-awesome": "^4.6.3",
    "intl": "^1.2.5",
    "leaflet": "^1.0.1",
    "leaflet.heat": "^0.2.0",
    "rxjs": "^5.0.1",
    "stubby": "^4.0.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2",
    "geojson-vt": "2.4.0",
    "leaflet.vectorgrid": "1.2.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^2.4.0",
    "@angularclass/hmr": "^1.2.2",
    "@types/jasmine": "2.5.38",
    "@types/leaflet": "^1.0.35",
    "@types/node": "^6.0.42",
    "codelyzer": "~2.0.0-beta.1",
    "coveralls": "^2.11.14",
    "download-cli": "^1.0.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-selenium-launcher": "^1.0.1",
    "karma-spec-reporter": "0.0.26",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor": "~5.1.0",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.1.0"
  }
